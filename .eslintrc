{
  "extends": "airbnb",
  "plugins": [
    "jsx-a11y",
    "import",
    "react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".js"
        ]
      }
    ],
    "react/destructuring-assignment": 0,
    "import/prefer-default-export": 0,
    "no-shadow": 0
  },
  "globals": {
    "describe": true,
    "expect": true,
    "fetch": true,
    "jest": true,
    "it": true,
    "navigator": true,
    "beforeAll": true,
    "beforeEach": true,
    "afterAll": true,
    "afterEach": true,
    "test": true,
    "jasmine": true,
    "__DEV__": true
  }
}